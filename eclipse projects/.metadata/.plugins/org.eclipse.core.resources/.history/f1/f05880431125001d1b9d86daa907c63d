package javaFeacture;
@FunctionalInterface
interface arithmatic
{
	public void addition(int a, int b);
	//public int subtraction(int c,int d);
	//public int multiplication(int e,int f);
	//public int divided(int p,int q);
}
public class LambdaEx {

	public static void main(String[] args) {
		arithmatic a1= (a,b) -> {System.out.println("Addition: "+(a+b));};
		a1.addition(20,30);
		
		/*arithmatic s1=(int c,int d)->{return(c-d);};
		System.out.println(s1.subtraction(50,20));
		
		arithmatic m1=(int e,int f)->{return(e*f);};
		System.out.println(s1.multiplication(5,2));
		
		
		*/

	}

}
